          "from": {
            "key_code": "0",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "amethyst_second_mode",
                "value": 1
              }
            }
          ],
          "to_after_key_up": [
