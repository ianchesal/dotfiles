{
  "title": "Amethyst Mode",
  "rules": [
    {
      "description": "Trigger Amethyst Mode/Amethyst Second Mode with 9/0",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "9",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "set_variable": {
                "name": "amethyst_mode",
                "value": 1
              }
            }
          ],
          "to_after_key_up": [
            {
              "set_variable": {
                "name": "amethyst_mode",
                "value": 0
              }
            }
          ],
          "to_if_alone": [
            {
              "key_code": "9"
            }
          ]
        },
        {
          "type": "basic",
          "conditions": [
            {
              "type": "variable_if",
              "name": "amethyst_mode",
              "value": 1
            }
